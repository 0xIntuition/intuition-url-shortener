{
  "name": "intuition-url-shortener",
  "version": "1.0.0",
  "description": "URL shortening service for Intuition protocol with social media meta tags",
  "main": "dist/server.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "hono",
    "url-shortener",
